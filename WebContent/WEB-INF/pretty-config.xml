<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces">

	<url-mapping id="index">
		<pattern value="/" />
		<view-id value="/faces/index.xhtml" />
	</url-mapping>
	<url-mapping id="servicos">
		<pattern value="/s.zelocallcenter" />
		<view-id value="/faces/servicos.xhtml" />
	</url-mapping>
	<url-mapping id="produtos">
		<pattern value="/p.zelocallcenter" />
		<view-id value="/faces/produtos.xhtml" />
	</url-mapping>
	<url-mapping id="localizacao">
		<pattern value="/l.zelocallcenter" />
		<view-id value="/faces/localizacao.xhtml" />
	</url-mapping>
	<url-mapping id="contato">
		<pattern value="/c.zelocallcenter" />
		<view-id value="/faces/contato.xhtml" />
	</url-mapping>
	
</pretty-config>